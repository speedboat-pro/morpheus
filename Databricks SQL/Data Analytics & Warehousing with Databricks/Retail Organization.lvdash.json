      "displayName": "Customers Table",
      "queryLines": [
        "USE CATALOG `labuser8664401_1745330262`;\n",
        "USE SCHEMA `dawd_v2`;\n",
        "\n",
        " SELECT * FROM customers;"
      ]
    },
    {
      "name": "d864b5b5",
      "displayName": "Total Sales",
      "queryLines": [
        "USE CATALOG `labuser8664401_1745330262`;\n",
        "USE SCHEMA `dawd_v2`;\n",
        "\n",
        " SELECT\n",
        "   sum(total_price) AS Total_Sales,\n",
        "   3000000 AS Sales_Goal\n",
        " FROM\n",
        "   sales;"
      ]
    },
    {
      "name": "1075650f",
      "displayName": "Sales Over Three Months",
      "queryLines": [
        "USE CATALOG `labuser8664401_1745330262`;\n",
        "USE SCHEMA `dawd_v2`;\n",
        "\n",
        " SELECT\n",
        "   customer_name,\n",
        "   total_price AS Total_Sales,\n",
        "   date_format(order_date, \"MM\") AS Month,\n",
        "   product_category\n",
        " FROM\n",
        "   sales\n",
        " WHERE\n",
        "   order_date >= to_date('2019-08-01')\n",
        " AND order_date <= to_date('2019-10-31');"
      ]
    },
    {
